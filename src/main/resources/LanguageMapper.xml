<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.webssm.mapper.LanguageMapper">

	<resultMap type="Language" id="language">
		<id property="language_id" column="language_id"/>
		<result property="name" column="name"/>
		<result property="last_update" column="last_update"/>
	</resultMap>

    <select id="select" resultMap="language">
    	select * from language
    </select>
    
</mapper>